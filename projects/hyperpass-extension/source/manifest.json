{
	"manifest_version": 2,
	"author": "Myles Trevino",
	"name": "Hyperpass",
	"version": "2021.5.30",
	"description": "A modern, unified password manager.",
	"homepage_url": "https://hyperpass.org",
	"icons":
	{
		"16": "16.png",
		"24": "24.png",
		"32": "32.png",
		"48": "48.png",
		"64": "64.png",
		"96": "96.png",
		"128": "128.png",
		"256": "256.png",
		"512": "512.png"
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"permissions": ["tabs", "activeTab", "contextMenus"],
	"browser_action":
	{
		"default_popup": "index.html",
		"default_icon":
		{
			"16": "16.png",
			"19": "19.png",
			"32": "32.png",
			"38": "38.png"
		}
	},
	"commands":
	{
		"_execute_browser_action":
		{
			"suggested_key": {"default": "Alt+H"},
			"description": "Activate extension"
		},
		"Autofill Username":
		{
			"suggested_key": {"default": "Alt+U"},
			"description": "Autofill username"
		},
		"Autofill Password":
		{
			"suggested_key": {"default": "Alt+P"},
			"description": "Autofill password"
		}
	},
	"background": {"page": "index.html"}
}
